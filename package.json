{
  "name": "tournament-vue",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:dev": "env-cmd -f .env.dev vue-cli-service serve",
    "serve:qa": "env-cmd -f .env.qa vue-cli-service serve",
    "serve:prod": "env-cmd -f .env.prod vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy:dev": "env-cmd -f .env.dev npm run build && s3-deploy './dist/**' --cwd './dist/' --region us-east-1 --bucket origin.dev.tournament.com --distId E1NU19TQY6EJMF --invalidate '/ /* *.* *' --profile client-tournament-dev --gzip --deleteRemoved",
    "deploy:qa": "env-cmd -f .env.qa npm run build && s3-deploy './dist/**' --cwd './dist/' --region us-east-1 --bucket origin.qa.tournament.com --distId E8JDIB65OL3AN --invalidate '/ /* *.* *' --profile client-tournament-qa --gzip --deleteRemoved",
    "deploy:prod": "env-cmd -f .env.prod npm run build && s3-deploy './dist/**' --cwd './dist/' --region us-east-1 --bucket origin.tournament.com --distId E3RYI5VJXFCAQ9 --invalidate '/ /* *.* *' --profile client-tournament-prod --gzip --deleteRemoved"
  },
  "dependencies": {
    "@formatjs/intl-displaynames": "^5.1.6",
    "@sanity/block-content-to-html": "^2.0.0",
    "@sanity/client": "^2.21.3",
    "@sanity/image-url": "^1.0.1",
    "@segment/in-eu": "^0.3.0",
    "@vue/composition-api": "^1.2.4",
    "aws-amplify": "^3.3.27",
    "aws-amplify-vue": "^2.1.5",
    "axios": "^0.19.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "core-js": "^3.6.4",
    "dayjs": "^1.10.5",
    "femtotween": "^1.1.2",
    "rollbar": "^2.19.2",
    "sendbird": "^3.0.125",
    "v-tooltip": "^2.1.3",
    "vue": "^2.6.11",
    "vue-avatar-cropper": "^1.0.6",
    "vue-concise-slider": "^3.4.4",
    "vue-debounce": "^2.5.6",
    "vue-router": "^3.1.6",
    "vue-svg-inline-loader": "^1.5.0",
    "vue-tel-input": "^5.6.2",
    "vue-tour": "^2.0.0",
    "vue2-touch-events": "^2.2.1",
    "vuejs-noty": "^0.1.3",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.3.0",
    "@vue/cli-plugin-eslint": "^4.3.0",
    "@vue/cli-plugin-router": "^4.3.0",
    "@vue/cli-plugin-vuex": "^4.3.0",
    "@vue/cli-service": "^4.3.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "env-cmd": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.12.0",
    "s3-deploy": "^1.4.0",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11",
    "webpack-bundle-analyzer": "^3.7.0"
  }
}
